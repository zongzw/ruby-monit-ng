config user group
    edit "SSO_Guest_Users"
    next
    edit "Guest-group"
        set member "guest"
    next
    edit "BMX-Global-Admin-Grp"
        set member "root_ldap_server"
            config match
                edit 1
                    set server-name "root_ldap_server"
                    set group-name "CN=Network-Admins,OU=bmxgroup,OU=system,OU=env,DC=bmxcn,DC=net"
                next
            end
    next
    edit "BMX-Global-Readonly-Grp"
        set member "root_ldap_server"
            config match
                edit 1
                    set server-name "root_ldap_server"
                    set group-name "CN=network-guest-group,OU=Groups,OU=Functional,DC=bmxcn,DC=net"
                next
            end
    next
