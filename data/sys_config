        set split-tunneling-routing-address "10.20.0.0/16_ALL10.20" "172.16.0.0/16_ALL_Baremetal_Internal" "172.17.0.0/16_ALL_cluster_Floating" "124.251.97.0/26_ALL_Bluebox_Openstack_API_Endpoint"
